#elmo-header {
    width: 100%;
    height: 80px;
}

#elmo-header {
    position: relative;
    z-index: 11;
    background-color: #34444f
}

a {
    color: #0081bd;
    text-decoration: none
}

body {
    font-size: 13px;
    font-family: "Helvetica";
    line-height: 20px;
    color: #000;
    background: white;
    margin: 0px;
}

#elmo-wrapper-container {
    display: inline;
    margin-left: 14%;

}

#elmo-topbar {
    position: absolute;
    top: 0px;
    right: -100px;
    width: 100%;
    overflow: hidden;
}

#elmo-identity {
    margin: 0px 13px 4px 0px;
    list-style: outside none none;

    float: right;
    font-size: 12px;
    padding: 0px 11px;
    background-color: #31383E;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
    border-top: 0px none;
}

#elmo-identity > li, a {
    display: inline;
    padding-right: 10px;
    color: white;
}

a {
    color: black;
}

a:focus, a:hover {
    color: black;
}

a:focus, a:hover {
    text-decoration: underline
}

.elmo-toplink {
    color: white;
    font-size: 13px;
}

.elmo-left-side {
    width: 50%;
    float: left;
}

#elmo-course_item_container {
    min-height: 530px;
    background-color: #e6e6e6;

    float: left;
    padding-left: 10px;

}

#elmo-semester {
    font-size: 2em;
    padding-bottom: 5px;
    border-bottom: thick solid red;
    padding-top: 5px;
    display: block;
    margin-left: 10px;
}

.elmo-course_item, .elmo-course_item > a {

    display: block;
    color: black;
    padding-left: 5px;
    font-size: 15px;
    padding-top: 3px;
    padding-bottom: 3px;
}

#content {
    background-color: white;
    margin-left: 14%;
    min-height: 530px;
    padding-top: 10px;
    padding-left: 10px;
    float: left;
}

#elmo-section-tabs-header {
    font-size: 2em;
    padding-bottom: 5px;
    border-bottom: thick solid red;
}

.elmo-section {

    display: block;
    background-color: #e6e6e6;
    padding-left: 10px;
    margin: 0px;
    font-size: 15px;
    padding-bottom: 6px;
    padding-top: 6px;

}

#elmo-header-inner {
    padding-top: 45px;
    max-width: 1100px;
    position: relative;
}

.elmo-header_image {
    float: left;
    display: inline;
    padding-top: 5px;
}

#topbar {
    right: -100px;
}

#elmo-menu {
    display: inline;
    margin-left: 120px;
}

.elmo-menu-active {

    border-bottom: thick solid yellow;
}

.elmo-menu-item, #course_menu_tab {
    color: white;
    display: inline;
    padding-left: 15px;
    padding-right: 15px;
    margin-right: 15px;
    font-size: 20px;
}

.elmo-menu-item > a {
    color: white;
    padding-left: 10px;
}

.elmo-active {
    background-color: white;
}

html {
    background-color: white;
}

.course_item {
    display: inline;
}

#content {
    margin-left: 30px;
    width: 80%;
    margin: auto;
}

#loginFailedMessage
{
    position: relative;
    top: 35px;
    left: 400px;
}

#loginbanner
{
    width: 100%;
    height: 15%;
    position: fixed;
    top: 40%;
    color: white;
    background-color: red;
}

#username
{
    position: relative;
    left: 30%;
    top: 50%;
    font-size: 1.5em;
    margin-top: 35px;
}

#password
{
    position: relative;
    left: 40%;
    top: 50%;
    font-size: 1.5em;
    margin-top: 35px;
}

#loginbutton
{
    position: relative;
    left: 50%;
    top: 50%;
    font-size: 1.5em;
    margin-top: 35px;
}

#elmo-login-image
{
    position: relative;
    left: 20%;
    top: 10%;
}

#menu {
    left: -25px;
    position: relative;
    width: 100%;
}

#courses_menu_item a.menu-item-title {
    width: 70px;
}

#menu .menu-item-title, #menu .menu-item-no-drop {
    color: #ffffff;
}

#calendar {
    max-width: 800px;
    max-height: 730px;

    font-family: "Lucida Grande",Helvetica,Arial,Verdana,sans-serif;
    font-size: 14px;

    display: block;

    padding-top: 35px;
    padding-left: 50px;
    margin-bottom: 50px;
}

